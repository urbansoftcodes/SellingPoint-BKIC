    <script>
        function ShowErrorMessage() {
            //if any page popup shows hide it before .
            $('#myModal').modal('hide');
            $(".modal-backdrop").remove();
            $('#errorModal').modal('show');

        }
        function closeErrorPopup() {
            $('#errorModal').modal('hide');
            $(".modal-backdrop").remove();
        }
       function ShowAuthorize() {
            var approved = confirm("Are you sure want to authorize this endorsement ?")
            if (approved) {
                showPageLoader();
                return true;
            }
            else {
                return false;
            }         
        }
          function ShowDelete() {
            var approved = confirm("Are you sure want to delete this endorsement ?")
            if (approved) {
                showPageLoader();
                return true;
            }
            else {
                return false;
            }          
        }
         function ShowClaimPopup() {
            $('#claimWarning').modal('show');
        }
        function CloseClaimPopup() {
            $('#claimWarning').modal('hide');
            $(".modal-backdrop").remove();
        }
    </script>
